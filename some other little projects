        "def column_correct(sudoku: list, column_no: int):\n",
        " l=[]\n",
        " a=0\n",
        " for row in sudoku:\n",
        "  if row[column_no]>0 and row[column_no] in l:\n",
        "    return False\n",
        "  else:\n",
        "   l.append(row[column_no])\n",
        " return True\n",
        "sudoku = [\n",
        "  [9, 0, 0, 0, 8, 0, 3, 0, 0],\n",
        "  [2, 0, 0, 2, 5, 0, 7, 0, 0],\n",
