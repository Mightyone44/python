        "com = 0\n",
        "soz = {}\n",
        "while com != 3:\n",
        " com = int(input(\"komut (1 ara, 2 ekle, 3 çık):\")) # Girişi integer'a dönüştürmek iyi olabilir\n",
        "\n",
        " if com == 1:\n",
        "  ad=input(\"isim:\")\n",
        "  if ad not in soz:\n",
        "     print(\"Böyle bir kayıt yok\")\n",
        "  else :\n",
        "     print(soz[ad])\n",
        "\n",
        " elif com == 2:\n",
        "   ad = input(\"isim:\")\n",
        "   num = input(\"numara:\")\n",
        "   # Check if the name already exists in the dictionary\n",
        "   if ad in soz:\n",
        "       # If it exists, append the new number\n",
        "       soz[ad] = soz[ad] + \",\" + num\n",
        "   else:\n",
        "       # If it doesn't exist, add the name and the number as a new entry\n",
        "       soz[ad] = num\n",
