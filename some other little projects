      "source": [
        "def oldest_person(people: list):\n",
        "  biggest_year = float('inf') # Initialize with a very large year\n",
        "  oldest_name = \"\" # Initialize with an empty string\n",
        "\n",
        "  for person in people:\n",
        "    name, year = person # Unpack the tuple\n",
        "    if year < biggest_year: # Compare years to find the oldest\n",
        "      biggest_year = year\n",
        "      oldest_name = name\n",
        "\n",
        "  return oldest_name # Return the name of the oldest person\n",
        "\n",
        "p1 = (\"Adam\", 1977)\n",
        "p2 = (\"Ellen\", 1985)\n",
        "p3 = (\"Mary\", 1953)\n",
        "p4 = (\"Ernest\", 1997)\n",
        "people = [p1, p2, p3, p4]\n",
        "\n",
        "print(oldest_person(people))"
      ],
