        "import random\n",
        "\n",
        "def lottery_numbers(amount: int, lower: int, upper: int):\n",
        "\n",
        "  dizi = []\n",
        "  while len(dizi) < amount: # Keep generating numbers until we have 'amount' unique numbers\n",
        "    generated_number = random.randint(lower, upper)\n",
        "    if generated_number not in dizi: # Check for uniqueness\n",
        "      dizi.append(generated_number)\n",
        "\n",
        "  dizi.sort() # Sort the list in ascending order\n",
        "  return dizi\n",
        "\n",
        "# Example usage based on the user's request\n",
        "for number in lottery_numbers(7, 1, 40):\n",
        "    print(number)\n",
        "\n",
        "print(lottery_numbers(5, 1, 49)) # Example from previous code"
      ],
