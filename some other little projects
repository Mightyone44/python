        "import string\n",
        "\n",
        "def separate_characters(my_string: str):\n",
        "  \"\"\"\n",
        "  Separates characters in a string into three strings based on their type:\n",
        "  ASCII letters, punctuation, and other characters.\n",
        "\n",
        "  Args:\n",
        "    my_string: The input string.\n",
        "\n",
        "  Returns:\n",
        "    A tuple containing three strings: ASCII letters, punctuation, and other characters.\n",
        "  \"\"\"\n",
        "  letters = \"\"\n",
        "  punctuation_chars = \"\"\n",
        "  others = \"\"\n",
        "\n",
        "  for char in my_string:\n",
        "    if char in string.ascii_letters:\n",
        "      letters += char\n",
        "    elif char in string.punctuation:\n",
        "      punctuation_chars += char\n",
        "    else:\n",
        "      others += char\n",
        "\n",
        "  return (letters, punctuation_chars, others)\n",
        "\n",
        "# Example usage:\n",
        "parts = separate_characters(\"Olé!!! Hey, are ümläüts wörking?\")\n",
        "print(parts[0])\n",
        "print(parts[1])\n",
        "print(parts[2])"
      ],
      "cell_type": "code",
