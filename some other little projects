        "def times_ten(start_index: int, end_index: int):\n",
        "  my_dict = {}\n",
        "  for i in range(start_index, end_index + 1):\n",
        "    my_dict[i] = i * 10\n",
        "  return my_dict\n",
        "\n",
        "# Example usage\n",
        "d = times_ten(3, 6)\n",
        "print(d)"
