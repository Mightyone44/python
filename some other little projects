        "from fractions import Fraction\n",
        "\n",
        "def fractionate(amount: int):\n",
        "  \"\"\"\n",
        "  Divides the number 1 into equal-sized fractions.\n",
        "\n",
        "  Args:\n",
        "    amount: The number of equal parts to divide 1 into.\n",
        "\n",
        "  Returns:\n",
        "    A list of Fraction objects representing the equal parts.\n",
        "  \"\"\"\n",
        "  fraction_list = []\n",
        "  for _ in range(amount):\n",
        "    fraction_list.append(Fraction(1, amount))\n",
        "  return fraction_list\n",
        "\n",
        "# Example usage\n",
        "for p in fractionate(3):\n",
        "    print(p)\n",
        "\n",
        "print()\n",
        "\n",
        "print(fractionate(5))"
