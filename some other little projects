        "def histogram(word):\n",
        "  b=[]\n",
        "  for i in range(len(word)):\n",
        "    if word[i] not in b:\n",
        "     b.append(word[i])\n",
        "     a=0\n",
        "     for j in range(len(word)):\n",
        "       if word[i]==word[j]:\n",
        "         a+=1\n",
        "     print(word[i],a*\"*\")\n",
        "histogram(\"statisticly\")\n"
