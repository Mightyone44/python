      "source": [
        "def count_matching_elements(my_matrix: list, element: int):\n",
        "  count=0;\n",
        "  for a in my_matrix:\n",
        "    for b in a:\n",
        "     if b==element:\n",
        "      count+=1\n",
        "  return count;\n",
        "m = [[1, 2, 1], [0, 3, 4], [1, 0, 0]]\n",
        "print(count_matching_elements(m, 0))"
