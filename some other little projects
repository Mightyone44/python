        "def who_won(game_board: list):\n",
        "  p1=0\n",
        "  p2=0\n",
        "  for row in game_board:\n",
        "    for col in row:\n",
        "      if col ==1:\n",
        "        p1+=1\n",
        "      elif col==2:\n",
        "        p2+=1\n",
        "  if p1>p2:\n",
        "    return \"Player 1\"\n",
        "  elif p2>p1:\n",
        "    return \"Player 2\"\n",
        "  else:\n",
        "    return \"Draw\"\n",
        "l = [[1,2,2],[1,0,2],[1,0,2]]\n",
        "print(who_won(l))"
      ],
