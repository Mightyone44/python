        "def row_sums(my_matrix: list):\n",
        "  for i in range(len(my_matrix)):\n",
        "    sum=0\n",
        "    for j in range(len(my_matrix[i])):\n",
        "      sum+=my_matrix[i][j]\n",
        "    my_matrix[i].append(sum)\n",
        "  return my_matrix\n",
        "print(row_sums([[1, 2, 3], [4, 5, 6], [7, 8, 9]]))"
      ],
